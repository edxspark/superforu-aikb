<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.dromara.kb.fileContent.mapper.FileContentMapper">

    <!-- 根据文件夹ID批量删除当前目录及下面文件夹和文件-->
    <delete id="deleteByFolderId">
        update kb_file_content set status = 1,link_cycle_id=#{cycleId}
        where catalog_ids like #{catalogIds}
    </delete>

</mapper>
